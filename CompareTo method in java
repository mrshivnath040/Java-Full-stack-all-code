public class Main {
    public static  void main(String[] args) {
        String result;
        List<String> countries = new LinkedList<>();
        countries.add("India");
        countries.add("USA");
        result = String.valueOf(countries.get(1).compareTo("India"));  //comperto method injava
        System.out.println(result);
    }
}
