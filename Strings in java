public class Main {
    public static void main(String[] args) {

        String a = "shivnath";
        String z = "gannu";
        String x = "dawke";
        String y = a.toUpperCase();
        String x1 = x.concat(z);
        System.out.println(x1);
        System.out.println(y);
        System.out.println(a);
        int b = a.length();
        System.out.println(b);

    }

}
