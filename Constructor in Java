// Vehicle calss
package org.studyeasy;

public class vehicle {

    private String color;
    private int speed;


    public vehicle() {
        color = "red";
        speed = 20;
    }

    public vehicle(String color,int speed) {
        this.color = color;
        this.speed = speed;

    }


    public String getColor() {
        return color;
    }

    public int getSpeed() {
        return speed;
    }
}
